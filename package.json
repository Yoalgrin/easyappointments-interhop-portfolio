{
  "name": "easyappointments",
  "description": "Open Source Web Scheduler",
  "homepage": "https://easyappointments.org",
  "author": "A.Tselegidis <alextselegidis@gmail.com>",
  "license": "GPL-3.0",
  "keywords": [
    "calendar",
    "scheduler",
    "appointments",
    "events",
    "dates",
    "google",
    "services"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alextselegidis/easyappointments.git"
  },
  "bugs": {
    "url": "https://github.com/alextselegidis/easyappointments/issues"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@fullcalendar/core": "^6.1.19",
    "@fullcalendar/daygrid": "^6.1.19",
    "@fullcalendar/moment": "^6.1.19",
    "@fullcalendar/timegrid": "^6.1.19",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.8",
    "cookieconsent": "^3.1.1",
    "flatpickr": "^4.6.13",
    "fullcalendar": "^6.1.11",
    "jquery": "^3.7.1",
    "jquery-jeditable": "^2.0.19",
    "jquery-migrate": "^3.5.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.48",
    "select2": "^4.1.0-rc.0",
    "tippy.js": "^6.3.7",
    "trumbowyg": "^2.31.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@prettier/plugin-php": "^0.21.0",
    "babel-preset-minify": "^0.5.2",
    "del": "^6.0.0",
    "fs-extra": "^11.1.0",
    "gulp": "^5.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-cached": "^1.1.1",
    "gulp-changed": "^4.0.3",
    "gulp-clean-css": "^4.3.0",
    "gulp-debug": "^4.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "prettier": "^3.0.0",
    "sass": "^1.57.1",
    "terser": "^5.44.0",
    "zip-dir": "^2.0.0"
  },
  "scripts": {
    "start": "npx --yes gulp",
    "build": "npx gulp build && npm run build:interhop-overrides",
    "docs": "npx gulp docs",
    "postinstall": "bash scripts/sync-vendors.sh",
    "vendors": "bash scripts/sync-vendors.sh",
    "verify:vendors": "bash scripts/sync-vendors.sh && echo vendors OK",
    "build:interhop-overrides": "npx terser assets/js/pages/interhop-account-override.js -m -c -o assets/js/pages/interhop-account-override.min.js && npx terser assets/js/pages/interhop-providers-override.js -m -c -o assets/js/pages/interhop-providers-override.min.js",
    "sync:vendors": "bash scripts/sync-vendors.sh",
    "assets:maps": "mkdir -p assets/vendor/bootstrap assets/vendor/tippy.js && cp node_modules/bootstrap/dist/js/bootstrap.min.js.map assets/vendor/bootstrap/ && cp node_modules/tippy.js/dist/tippy-bundle.umd.min.js.map assets/vendor/tippy.js/"
  }
}
